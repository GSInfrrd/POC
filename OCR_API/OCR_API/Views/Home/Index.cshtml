@{
    ViewBag.Title = "Home Page";
}


<div class="row">
    <div class="col-md-4">
        <h2>Upload Image</h2>
        @using (Html.BeginForm("UploadFile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
		{

			<div>
				@Html.TextBox("file", "", new { type = "file" }) <br />

				<input type="submit" value="Upload" />

				@ViewData["Result"]
				@ViewBag.Message

			</div>


		}
		
    </div>
    
</div>